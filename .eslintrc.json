{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb", "plugin:react/recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "linebreak-style": "off",
    "no-unused-vars": "warn",
    "import/prefer-default-export": "warn"
  }
}
